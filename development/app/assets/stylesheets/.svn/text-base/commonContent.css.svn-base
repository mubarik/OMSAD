img.logo {
   float:left;
}

.pageHeadingBar {
    padding: 18px 2%;
}

.breadcrumb {
    border-bottom:1px solid #ccc;
    line-height: 23px;
    font-size:13px;
    font-weight:bold;
    padding-left:8px;
}

.breadcrumb a:link, .breadcrumb a:visited { 
    text-decoration:none;
    color:#003366;
}

.breadcrumb a:hover, .breadcrumb a:focus { 
    text-decoration:underline;
    color:#0099CC;
}

.breadcrumb {
    display: none;
    color:#FF9900;
}

.invalidEntry {
    /*making text box with clien`t side valiation error visible*/
   background-color: salmon;
}


.inputBoxPlacement {
    /*where input box is placed*/
    padding: 3px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}


h1, h2, h3, .labelDefinitionTerm, .labelDefinitionDetails {
    /*making website text non-selectable*/
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -o-user-select: none;
    user-select: none;
}


.fieldErrorPlacement {
    /*where validation errors are displayed*/
     padding-left: 5px;
     color: red;
     font-weight: normal;
}

.popupfieldErrorPlacement {
    /*where validation errors are displayed*/
     padding-left: 5px;
     color: red;
     font-weight: normal;
     width: 170px;
}


.dynamicFieldErrorPlacement
{
    padding-left: 10px;
    color:red;
}

h2 .dynamicFieldErrorPlacement
{
    padding-left: 10px;
    color:red;
    font-weight: normal;
    font-size: 13px;
}


input[type="submit"], button, .standardButtonSize {
    width: 110px;
}

.autoWidth {
    width:auto;
}

.dropdown {
    /*select combo boxes*/
    width:355px;
}



.uidatePicker, .uienddatePicker,.uidayMonthPicker, .inputTextbox, .telephone,.inputTextboxSt,.inputTextboxEd {
    margin: 0;
    padding: 0;
    width:350px;
}


h1 {         /*page heading*/
    font-size:2.1em;
    background:transparent;
    text-transform:capitalize;
    letter-spacing:0.02em;
    font-weight: bolder;
    margin:0;
    padding:0;
}


h2 {
    font-size:1.5em;
    margin: 4px 0 0 2%;
    padding: 0;
}


h3 {
    text-transform:capitalize;
    font-size:1.6em;
    margin-top: 0;
    margin-bottom: 5px;
}


h3 a {
    text-decoration: underline;
    color: black;
}


h3 a:visited {
    text-decoration: underline;
    color: black;
}


h3 a:hover {
    text-decoration: underline;
    color: blue;
}

h4 {
    text-transform:capitalize;
    font-size:1.0em;
    margin-top: 0;
    margin-left: 2px;
    margin-bottom: 2px;   
}
hr {
    margin: 0;
    padding: 0;
}



.messages {
    padding-left: 28px;
    color: green;
    font-size: 14px;
    background-image:url('iconTick.png');
    background-repeat: no-repeat;
    background-position: left;
    float:left;
    margin-left: 2%;
    margin-top: -18px;
}

.errors {
    padding-left: 28px;
    color: red;
    font-size: 14px;
    background-image: url('Error.png');
    background-repeat: no-repeat;
    background-position: left;
    float:left;
    margin-left: 2%;
    margin-top: -18px;

}


.headingLineBreak {
    border-width: 2px;
    margin-bottom: 20px;
}


.dataBlock {        /*a block of data that may be repeated many times on the page one below the other*/
    padding: 10px 0%;
    margin: 1px auto;
    border: 1px solid #000;
    -moz-border-radius: 5px;
    border-radius: 5px;
    display: inline-block;
    width:99.8%;
}


.dataBlockFaculty {        /*a block of data that may be repeated many times on the page one below the other*/
    border: 1px solid #000;
    -moz-border-radius: 5px;
