<div class="expander">
  <%=p.fields_for :conference_publication do |c|%>
  <table>
    <tr>
      <td class="labelDefinitionTerm">Conference Name: <span class="requiredFieldAsterisk">*</span></td>
      <td class="inputBoxPlacement">
           <%= c.text_field :conferencename,
                                           :id=>"txconferencename",
                                           :required =>true,
                                           :title => "Specical characters are not allowed",
                                           :class=>"inputTextbox"%>
      </td>
      <td class ="fieldErrorPlacement">
        <%= @publication.conference_publication.errors[:conferencename][0] %>
      </td>
    </tr>

    <tr>
      <td class="labelDefinitionTerm">WebSite: <span class="requiredFieldAsterisk">*</span></td>
      <td class="inputBoxPlacement">
          <%= c.text_field  :website,
                                           :id=>"txconferencewebsite",
                                           :required =>true,
                                           :title => "Enter the website of conference e.g http://www.lums.edu.pk",
                                           :class=>"inputTextbox"%>
      </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:website][0] %>
      </td>
    </tr>

    <tr>
      <td class="labelDefinitionTerm">Start Date: </td>
      <td class="inputBoxPlacement">
          <%= c.text_field  :conferencestartdate,
                                           :id=>"txstartdate",
                                           :class=>"uidatePicker"%>
      </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:startdate][0] %>
      </td>
    </tr>

    <tr>
      <td class="labelDefinitionTerm">End Date: </td>
      <td class="inputBoxPlacement">
         <%= c.text_field  :conferenceenddate,
                                           :id=>"txenddate",
                                           :class=>"uidatePicker"%>
      </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:enddate][0] %>
      </td>
    </tr>

    <tr>
      <td class="labelDefinitionTerm">Rank: </td>
          <td>
            <%=c.collection_select  :rank_id,Rank.all,:id,:name,
              options={:prompt=>"Select a Rank",
              :selected=>c.object.rank_id},              
              :title   => "Please specify a Rank",
              :class=>"dropdown"%>
          </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:rank][0] %>
      </td>
    </tr>




      <tr>
           <td class="labelDefinitionTerm">Ranking Year: </td>
            <td>
                <%= c.select :year, 1947..Time.now.year, {} , :class => "dropdown"  %>
            </td>
             <td class ="fieldErrorPlacement">
                    <%= @publication.conference_publication.errors[:year][0] %>
             </td>
       </tr>

       <tr>
            <td class="labelDefinitionTerm">Ranking Reference: </td>
             <td class="inputBoxPlacement">
                  <%= c.text_field  :reference,
                                :id=>"txconferencereference",
                                :title => "Specical characters are not allowed",
                                :class=>"inputTextbox"%>
              </td>
            <td class ="fieldErrorPlacement">
                              <%= @publication.conference_publication.errors[:reference][0] %>
           </td>
      </tr>
 
 




    <%#*<tr>%>
      <%#*<td class="labelDefinitionTerm">Ranking Year: </td>%>
      <%#*<td>%>
        <%#= c.select :year, 1947..Time.now.year, {} , :class => "dropdown"  %>
      <%#*</td>%>
      <%#*<td class ="fieldErrorPlacement">%>
         <%#= @publication.conference_publication.errors[:year][0] %>
      <%#*</td>%>
    <%#*</tr>%>


    <%#*<tr>%>
      <%#*<td class="labelDefinitionTerm">Ranking Reference: </td>%>
      <%#*<td class="inputBoxPlacement">%>
          <%#= c.text_field  :reference,
                                           :id=>"txconferencereference",
                                           :title => "Specical characters are not allowed",
                                           :class=>"inputTextbox"%>
      <%#*</td>%>
      <%#*<td class ="fieldErrorPlacement">%>
         <%#= @publication.conference_publication.errors[:reference][0] %>
      <%#*</td>%>
    <%#*</tr>%>
    <tr>
      <td class="labelDefinitionTerm">Reference Website: </td>
      <td class="inputBoxPlacement">
          <%= c.text_field  :referenceweb,
                                           :id=>"txconferencereferenceweb",
                                           :title => "Enter the website of reference website e.g http://www.lums.edu.pk",
                                           :class=>"inputTextbox"%>
      </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:referenceweb][0] %>
      </td>
    </tr>
    <tr>
      <td class="labelDefinitionTerm">Acceptance Rate(%): </td>
      <td class="inputBoxPlacement">
          <%= c.text_field  :acceptancerate,
                                           :id=>"txconferenceacceptancerate",
                                           :title => 'Enter digits between 1 to 100 e.g 60',
                                           :class=>"inputTextbox"%>
      </td>    
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:acceptancerate][0] %>
      </td>
    </tr>

    <tr>
      <td class="labelDefinitionTerm">City: </td>
      <td class="inputBoxPlacement">
        <%= c.text_field     :city,
                                      :id=>"txconferencecity",
                                      :title => "Only alphabets are allowed e.g Rawalpindi",
                                      :class=>"inputTextbox"%>
      </td>
      <td class ="fieldErrorPlacement">
         <%= @publication.conference_publication.errors[:city][0] %>
      </td>
    </tr>

  </table>
 <%end%>
</div>