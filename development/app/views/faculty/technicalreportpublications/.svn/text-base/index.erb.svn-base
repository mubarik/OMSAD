<%= content_for :content do  %>
  <div class="pageHeadingBar">
    <h1>Technical Reports</h1>
  </div>
<hr class="headingLineBreak"/>
    <%unless @technicalreport_publication_wrappers.blank?%>
       <% if !flash[:success].blank? %>
        <div class="messages">
          <p><%=flash[:success]%></p>
        </div>
       <%end%>
      <%=render   :partial=>"layouts/applicationindex" ,
            :collection=>@technicalreport_publication_wrappers,
            :as=>:wrapper%>
    <%else%>
       <div class="dataBlock">
          <div class="dataPadder">
            No technical report is available right now. To continue, please <%= link_to "add a technical report ", :controller => "technicalreportpublications", :action => "new"%>
          </div>
       </div>
     <%end%>
<%end%>

<% content_for :menu do %>
  <%= render "faculty/navigationmenu" %>
<%end%>
