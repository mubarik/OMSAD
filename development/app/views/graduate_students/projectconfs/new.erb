      <div class="bottomButtons">
    <%#= button_tag "Cancel", :action => "index", :id => "students" %>
        <%=submit_tag "Save", :id=>"saveButton"%>
      </div>
    </div>

  <%end%>
<%end%>
<% content_for :menu do %>
  <%= render "graduate_students/navigationmenu" %>
<%end%>