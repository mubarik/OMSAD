<% content_for :menu do %>
  <%= render "graduate_students/navigationmenu" %>
<% end %>

<% content_for :content do %>


  <div class="expandingBlock">

    <div class="pageHeadingBar">
      <h1>Import Students</h1>
    </div>
    <hr class="headingLineBreak"/>
      <% if !flash.now[:notice].blank? %>
      <div class="messages">
        <p><%=flash.now[:notice]%></p>
      </div>
      <div class="clearFloatingAndStartFromBelow"></div>
      <%end%>

     <% if !flash.now[:msgRed].blank? %>
          <div class="errors">
            <p><%=flash.now[:msgRed]%></p>
          </div>
          <div class="clearFloatingAndStartFromBelow"></div>
        <%end%>
    <%#Student CSV Upload Expander%>

    <div class="expanderHeaderBar" id="basicInfoExpander">
      <%=image_tag("", :alt=>'', :class=>'expanderDownButton')%>

      <h2>Upload Students</h2>

      <div class="expander">

        <%= nested_form_for @uploadcheck ,:html=>{:id=>"csvimportform",
                                            :class=>'clientSideValidatedForm',
                                            :multipart=>true},
                                            :url =>{ :controller => "GraduateStudents/students",
                                            :action => "uploadfile"} do |f| %>
<table>
           <tr>
             <td class="labelDefinitionTerm"><label > Select a CSV File :</label></td>
         <td class="inputBoxPlacement"><%= f.file_field :uploadfile  %></td>
             <td class="fieldErrorPlacement"></td>
           </tr>
          </table>
        <div class="bottomButtons" style="margin:0px;">
          <table>
            <tr>
              <td></td>
              <td>  <%= f.submit "Upload" , :id=> 'student' %></td>
            </tr>
          </table>
        </div>

  <%end%>


