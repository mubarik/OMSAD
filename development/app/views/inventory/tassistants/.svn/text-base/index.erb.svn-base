<% content_for :menu do %>
        <%= render "inventory/navigationmenuRATA" %>
<% end %>

<%= content_for :content do  %>
       <div class="pageHeadingBar">
          <h1>Teaching Assistant</h1>
      </div>
      <hr class="headingLineBreak"/>
       <% if !flash[:msgGreen].blank? %>
        <div class="messages">
          <p><%=flash[:msgGreen]%></p>
        </div>
       <%end%>

      <%unless @tassistant_list_wrappers.blank?%>
       <%=render :partial=>"layouts/applicationindex" ,
                 :collection=>@tassistant_list_wrappers,
                 :as=>:wrapper%>
      <%else%>
        <div class="dataBlock">
          <div class="dataPadder">
            No teacher assistant has been added yet. To continue, please <%= link_to "add teacher assistant", :controller => "tassistants", :action => "new"%>
          </div>
        </div>
      <%end%>

<%end%>